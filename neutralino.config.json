{
  "applicationId": "com.mochki.gameboy-tools",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "gameboy-tools",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "gameboy-tools",
    "resourcesPath": "/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/public/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
      "patchFile": "/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
